{
  "expo": {
    "name": "Tabletop Organizer",
    "slug": "TabletopVault",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "tabletopvault",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#991b1b"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tabletopvault.app",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "Tabletop Organizer needs camera access to take photos of your miniatures.",
        "NSPhotoLibraryUsageDescription": "Tabletop Organizer needs photo library access to select images for your collection.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "Tabletop Organizer needs photo library access to select images for your collection.",
          "cameraPermission": "Tabletop Organizer needs camera access to take photos of your miniatures."
        }
      ],
      "@react-native-community/datetimepicker",
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "07c1e277-32dd-445c-a320-0487f9f6e3c5"
      }
    }
  }
}
